          <ScalarProperty Name="Notes" ColumnName="Notes" />
        </MappingFragment>
      </EntityTypeMapping>
    </EntitySetMapping>
    <EntitySetMapping Name="Items">
      <EntityTypeMapping TypeName="FACTURAModel.Item">
        <MappingFragment StoreEntitySet="Items">
          <ScalarProperty Name="ProductId" ColumnName="ProductId" />
